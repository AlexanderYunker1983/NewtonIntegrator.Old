        <TextBox Margin="5" Grid.Column="3" Grid.Row="8" Width="150" TextAlignment="Right">
            <TextBox.Text>
                <Binding Path="M2" UpdateSourceTrigger="PropertyChanged">

                </Binding>
            </TextBox.Text>
        </TextBox>
